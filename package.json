{
  "name": "feedolin",
  "version": "1.1.0",
  "source": "application/index.html",
  "author": "strukturart",
  "license": "MPL-2.0",
  "browserslist": [
    "Firefox 48"
  ],
  "targets": {
    "default": {
      "context": "browser",
      "outputFormat": "global",
      "includeNodeModules": true,
      "isLibrary": false,
      "engines": {
        "browsers": [
          "Firefox 48"
        ]
      }
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "firefox": "48"
          },
          "useBuiltIns": "usage",
          "corejs": "3.37"
        }
      ]
    ]
  },
  "scripts": {
    "dev": "parcel dist/index.html",
    "build": "rm -rf dist/* && parcel build  --no-source-maps application/index.html && cp application/manifest.webapp dist && mkdir -p dist/assets/js && cp -r application/assets/icons dist/assets/ && cp application/assets/js/kaiads.v5.min.js dist/assets/js/",
    "run": "parcel",
    "web": "rm -rf docs/* && parcel build --dist-dir docs --public-url ./ --no-source-maps --no-content-hash application/index.html && cp application/manifest.webapp docs && cp application/manifest.webmanifest docs && mkdir -p docs/assets && cp -r application/assets/icons docs/assets/ && mkdir -p docs/assets/js/ && cp application/assets/js/kaiads.v5.min.js docs/assets/js/ &&  cp -r application/assets/fonts docs/assets/&& ./generate_file_list.sh"
  },
  "devDependencies": {
    "@agragregra/parcel-namer-beautify": "^1.0.1",
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-named-capturing-groups-regex": "^7.17.12",
    "@babel/preset-env": "^7.27.2",
    "@parcel/packager-raw-url": "2.12.0",
    "@parcel/resolver-default": "2.12.0",
    "@parcel/transformer-html": "2.12.0",
    "assert": "^2.1.0",
    "babel-plugin-transform-async-to-promises": "^0.8.18",
    "buffer": "^6.0.3",
    "core-js": "^3.45.1",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^16.4.1",
    "es-check": "^9.1.2",
    "events": "^3.3.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "parcel": "^2.16.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "source-map-explorer": "^2.5.3",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "string_decoder": "^1.3.0",
    "svgo": "^3.3.2",
    "timers-browserify": "^2.0.12",
    "url": "^0.11.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.6",
    "@parcel/service-worker": "^2.8.0",
    "dayjs": "^1.11.13",
    "dompurify": "^3.0.3",
    "localforage": "^1.10.0",
    "mithril": "^2.0.4",
    "regenerator-runtime": "^0.14.1",
    "rss-parser": "^3.13.0",
    "swiped-events": "1.2.0",
    "url-search-params-polyfill": "^8.1.1",
    "uuid": "^9.0.0"
  }
}
